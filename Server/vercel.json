{
    "version": 2,
    "builds": [
        {
            "src": "7k-dataset-api.py",
            "use": "@vercel/python"
        },
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "7k-dataset-api.py"
        },
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}